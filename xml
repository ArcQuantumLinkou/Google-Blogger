     <script type='text/javascript'>
     //<![CDATA[
    
(function($, window, document){
  'use strict';

  // ===== 全域設定（可被外部覆寫） =====
  var CFG = {
    perPage: typeof window.perPage === 'number' ? window.perPage : 12,
    monthNames: window.month_format || ['Jan','Feb','Mar','Apr','May','Jun','Jul','Aug','Sep','Oct','Nov','Dec'],
    noImage: window.no_image || 'https://blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEiRGu6b6dVmXsI0MmtlVOI7Y92dETTi00ZP1XY80Ra-xBjiEh1l7B3p5eOc8SppPYMqiBjIquO_ehpXIvkOARBOfZ0fF-LCeYD_sW13sw459BXAC2QNdLgjNkiCs-Gro2O4qV-rKPbzfYkY/s1600/notfound.png',
    enableCommentsTab: typeof window.cmtal === 'boolean' ? window.cmtal : true
  };
  window.perPage = CFG.perPage; // 讓外部或舊分頁碼能取得一致數值

  // ===== 小工具 =====
  var U = {
    normalizeImgUrl: function(url){
      if(!url) return CFG.noImage;
      var u = String(url);
      if (u.indexOf('/hqdefault.jpg') !== -1) u = u.replace('/hqdefault.jpg','/mqdefault.jpg');
      if (u.indexOf('/default.jpg')   !== -1) u = u.replace('/default.jpg','/mqdefault.jpg');
      if (u.indexOf('/s72-c')         !== -1) u = u.replace('/s72-c','/s1600');
      if (u.indexOf('/w72-h72-p-nu')  !== -1) u = u.replace('/w72-h72-p-nu','/s1600');
      return u;
    },
    bgCover: function(url){ return 'background:url(' + url + ') no-repeat center center;background-size:cover'; },
    formatDate: function(iso){
      if(!iso) return '';
      var y = iso.substring(0,4);
      var m = parseInt(iso.substring(5,7),10)-1;
      var d = iso.substring(8,10);
      return (CFG.monthNames[m] || '') + ' ' + d + ', ' + y;
    },
    htmlToText: function(html){ return $('<div>').html(html || '').text(); },
    jsonp: function(url, success, error){
      $.ajax({ url: url, type: 'GET', dataType: 'jsonp' }).done(success).fail(error || function(){});
    },
    safe: function(fn){
      try { fn(); } catch(e){ /* 靜默失敗，避免阻斷整體功能 */ }
    }
  };

  // ===== SelectNav 初始化 =====
  function initSelectNav(id){
    if (typeof window.selectnav === 'function'){
      U.safe(function(){ window.selectnav(id); });
    }
  }

  // ===== 主選單（Alpha Menu）重建 =====
  function buildAlphaMenu(){
    var $wrap = $('#alpha-menu');
    var $lis = $wrap.find('ul > li');
    if(!$lis.length) return;

    var last = -1;
    var flatHtml = '';

    $lis.each(function(){
      var $li = $(this);
      var text = $li.text() || '';
      var url  = $li.find('a').attr('href') || '#';

      var level = 0, x = 0, z = 0;
      for(; z < text.length; z++){
        x = text.indexOf('_', x);
        if (x === -1) break;
        level++; x++;
      }

      if (level > last) flatHtml += '<ul>';
      if (level < last){
        var off = last - level;
        for (var i=0; i<off; i++){ flatHtml += '</ul></li>'; }
      }

      text = text.replace(/_/g, '');
      flatHtml += '<li><a href="' + url + '">' + text + '</a>';
      last = level;
    });

    for (var i=0; i<=last; i++){ flatHtml += '</ul>'; if(i!==0) flatHtml += '</li>'; }

    $('#alpha-menu .LinkList').html(flatHtml);
    $('#alpha-menu > .LinkList > ul').attr('id','alpha_menu');

    $('#alpha_menu a').each(function(){
      var $a = $(this);
      var href = ($a.attr('href') || '').toLowerCase();
      if (href.indexOf('[mega menu]') !== -1){
        $a.addClass('mega-link');
        $a.attr('href', '/search/label/' + $a.text() + '?&max-results=' + CFG.perPage);
      }
    });

    initSelectNav('alpha_menu');
    $('#alpha-menu ul > li > ul').parent('li').addClass('hasSubmenu');
    $('#alpha-menu .widget').attr('style','display:block!important;');
  }

  // ===== Mega Menu，依標籤載入文章 =====
  function initMegaMenu(){
    $('#alpha_menu a.mega-link').each(function(){
      var $link = $(this);
      var label = $link.text();
      var feed  = '/feeds/posts/default/-/' + label + '?alt=json-in-script&max-results=4';

      U.jsonp(feed, function(data){
        var $li = $link.closest('li');
        $li.addClass('parent isMega hasSubmenu');

        var alt = (data.feed.link || []).find(function(l){ return l.rel === 'alternate' && l.type === 'text/html'; });
        if (alt) $link.attr('href', alt.href + '?&max-results=' + CFG.perPage);

        var total = Number(data.feed.openSearch$totalResults && data.feed.openSearch$totalResults.$t || 0);
        var start = Number(data.feed.openSearch$startIndex && data.feed.openSearch$startIndex.$t || 1);
        var step  = Number(data.feed.openSearch$itemsPerPage && data.feed.openSearch$itemsPerPage.$t || 4);
        var stages = Math.max(1, Math.ceil(total / step));

        if (!data.feed.entry) return;

        var makeItem = function(entry){
          var cat = entry.category && entry.category[0] && entry.category[0].term || '';
          var author = entry.author && entry.author[0] && entry.author[0].name && entry.author[0].name.$t || '';
          var content = entry.content && entry.content.$t || '';
          var img = CFG.noImage;

          if (entry.media$thumbnail && entry.media$thumbnail.url){
            img = U.normalizeImgUrl(entry.media$thumbnail.url);
            if (img.indexOf('img.youtube.com') !== -1) img = img.replace('/default.jpg','/maxresdefault.jpg');
          }else if (content.indexOf('<img') > -1){
            img = U.normalizeImgUrl($('<div>').html(content).find('img:first').attr('src'));
          }

          var title = entry.title && entry.title.$t || '';
          var url = '#';
          (entry.link || []).forEach(function(l){ if (l.rel === 'alternate') url = l.href; });

          return '' +
          '<div class="alpha-b-mega-item col-sm-3">' +
            '<div class="alpha-b-m-thumb" style="' + U.bgCover(img) + '"><a class="linkcover" href="' + url + '"></a></div>' +
            '<div class="entry-header"><div class="article-meta">' +
              '<span class="post-cat"><a href="/search/label/' + cat + '?&max-results=' + CFG.perPage + '" title="View all posts in ' + cat + '">' + cat + '</a></span>' +
              '<span class="posted-on"><i class="ion-ios-person-outline"></i><a href="' + url + '" rel="bookmark">' + author + '</a></span>' +
            '</div></div>' +
            '<div class="alpha-b-text"><a href="' + url + '">' + title + '</a></div>' +
          '</div>';
        };

        var $mega = $('<ul ' +
          'data-itemnums="'+step+'" data-label="'+label+'" data-start="'+start+'" data-stages="'+stages+'" data-cstage="1" data-tpst="'+total+'">' +
          '<div class="mega-nav">' +
            '<a class="mega-prev disable" href="javascript:;"><i class="ion-android-arrow-back"></i></a>' +
            '<a class="mega-next' + (total > step ? '' : ' nav-disable') + '" href="javascript:;"><i class="ion-android-arrow-forward"></i></a>' +
          '</div>' +
          '<div class="alpha-b-mega-loading" style="display:none"><div class="alpha-b-loading-inner"><div class="alpha-loader"></div></div></div>' +
          '<div class="mega-inner row"></div>' +
        '</ul>');

        $link.after($mega);
        var $inner = $mega.find('.mega-inner');
        $inner.html(data.feed.entry.map(makeItem).join(''));

        var loadStage = function(next){
          var $ul = $mega;
          var l = $ul.data('label');
          var s = Number($ul.data('start'));
          var c = Number($ul.data('cstage'));
          var m = Number($ul.data('stages'));
          var n = Number($ul.data('itemnums'));

          $ul.find('.alpha-b-mega-loading').fadeIn(0);
          s = next ? s + n : s - n;
          var newStage = next ? c + 1 : c - 1;
          var url = '/feeds/posts/default/-/' + l + '?alt=json-in-script&start-index=' + s + '&max-results=' + n;

          U.jsonp(url, function(resp){
            $ul.data('start', s);
            $ul.data('cstage', newStage);
            if (resp.feed.entry){
              $inner.html(resp.feed.entry.map(makeItem).join(''));
            }
            setTimeout(function(){ $ul.find('.alpha-b-mega-loading').fadeOut(); }, 200);
            $ul.find('.mega-prev').toggleClass('disable', newStage <= 1);
            $ul.find('.mega-next').toggleClass('disable', newStage >= m);
          }, function(){
            setTimeout(function(){ $ul.find('.alpha-b-mega-loading').fadeOut(); }, 200);
          });
        };

        $mega.find('.mega-prev').on('click', function(e){ e.preventDefault(); if(!$(this).hasClass('disable')) loadStage(false); });
        $mega.find('.mega-next').on('click', function(e){ e.preventDefault(); if(!$(this).hasClass('disable')) loadStage(true); });
      });
    });
  }

  // ===== 特色文章（Owl Carousel） =====
  function initFeatured(){
    $('.alpha_featured_widget .HTML .widget-content').each(function(){
      var $box = $(this);
      var $span = $box.find('span');
      var label = $span.attr('data-label');
      var slides = Number($span.attr('data-slides') || 4);
      var parentId = $box.parent().attr('id');
      var boxType = $span.attr('data-type') || '';
      if (!boxType || boxType.indexOf('alphafeatured') === -1 || !label) return;

      var url = '/feeds/posts/default/-/' + label + '?alt=json-in-script&max-results=' + slides;
      U.jsonp(url, function(data){
        var html = '<div class="alpha-featured owl-carousel">';
        if (data.feed.entry){
          data.feed.entry.forEach(function(entry){
            var content = entry.content && entry.content.$t || '';
            var img = CFG.noImage;
            if (content.indexOf('//www.youtube.com/embed/') > -1){
              img = U.normalizeImgUrl(entry.media$thumbnail && entry.media$thumbnail.url || '');
            } else if (content.indexOf('<img') > -1){
              img = U.normalizeImgUrl($('<div>').html(content).find('img:first').attr('src'));
            }
            var title = entry.title && entry.title.$t || '';
            var link = '#';
            (entry.link || []).forEach(function(l){ if(l.rel === 'alternate') link = l.href; });
            html += '' +
              '<div class="alpha-hero">' +
                '<div class="alpha-featured-image">' +
                  '<div class="alphafeatured-thumb"><a class="blsm-img" href="' + link + '" style="' + U.bgCover(img) + '"></a></div>' +
                  '<div class="alpha-hero-content"><div class="body-row midelizer slideyrow"><div></div>' +
                    '<h3 class="alphafeatured-title"><a href="' + link + '">' + title + '</a></h3>' +
                  '</div></div>' +
                '</div>' +
              '</div>';
          });
        }else{
          html += '<div class="alpha-empty-panel"><div class="alpha-empty-thumb"><img src="' + CFG.noImage + '"></div><div class="alpha-empty-text"><h3>No posts found for label: "' + label + '"</h3></div></div>';
        }
        html += '</div>';

        $('.alpha_featured_widget .HTML .widget-content').each(function(){
          if ($(this).parent().attr('id') === parentId){
            $(this).html(html);
            $(this).parent().addClass('alphafeatured alpa');
            var $carousel = $(this).find('.alpha-featured.owl-carousel');
            if ($.fn.owlCarousel){
              try {
                $carousel.owlCarousel({ margin:0, loop:true, items:1, nav:true, dots:false, navText:['',''] });
              } catch(e) {
                $carousel.removeClass('owl-carousel').show();
              }
            } else {
              $carousel.removeClass('owl-carousel').show();
            }
          }
        });
      });
    });
  }

  // ===== 區塊搬移 =====
  function relocateWidgets(){
    function move($from, beforeSel){
      var html = $from.html();
      if (html){ $(beforeSel).before(html); $from.html(''); }
    }
    move($('.post-author-social .widget-content'), 'span[name="author-social"]');
    move($('.post-author-widget .widget-content'), 'span[name="author-post"]');
    move($('#alpha-advert-2 .widget-content'), 'a[name="ad-post-top"]');
    move($('#alpha-advert-3 .widget-content'), 'a[name="ad-post-bottom"]');
  }

  // ===== 縮圖正規化 =====
  function normalizeThumbs(){
    $('.post-home-image .post-thumb a').each(function(){
      var style = $(this).attr('style') || '';
      var m = /url\(([^)]+)\)/.exec(style);
      if (m && m[1]){
        var clean = U.normalizeImgUrl(m[1].replace(/['"]/g,''));
        $(this).attr('style', style.replace(/url\(([^)]+)\)/, 'url(' + clean + ')'));
      }
    });
    $('.PopularPosts ul li img').attr('src', function(_i, src){
      return U.normalizeImgUrl(src).replace('notfound.pngg','notfound.png');
    });
  }

  // ===== 特色文章 overlay =====
  function enhanceFeaturedOverlay(){
    $('.FeaturedPost .post-summary').each(function(){
      var $t = $(this);
      var u = $t.find('.image').attr('src');
      if (u) $t.attr('style', U.bgCover(u));
      if (!$t.find('.overlay').length) $t.append('<div class="overlay"></div>');
    });
  }

  // ===== 側邊欄分頁籤 =====
  function initSideTabs(){
    var t1 = $('#alpha-sidebar-tabs #tabside1 .widget h2').text();
    $('.alpha-sidebar-tab .item-1 a').text(t1);
    var t2 = $('#alpha-sidebar-tabs #tabside2 .widget h2').text();
    $('.alpha-sidebar-tab .item-2 a').text(t2);
    var t3 = $('#alpha-sidebar-tabs #tabside3 .widget h2').text();
    $('.alpha-sidebar-tab .item-3 a').text(t3);
    $('#tabside1 .widget h2,#tabside2 .widget h2,#tabside3 .widget h2,#tabside1 .widget-title,#tabside2 .widget-title,#tabside3 .widget-title').remove();
    $('.alpha-sidebar-tab li').addClass('alpha-hide');

    if ($.fn.tabslet){
      $('.alpha-sidebar-tabs').tabslet({ mouseevent:'click', attribute:'href', animation:true });
    }
    if (CFG.enableCommentsTab && $.fn.simplyTab){
      $('.alpha-tabs').simplyTab({ active:1, fx:'fade', showSpeed:400, hideSpeed:400 });
      $('.alpha-blogger-comment').append($('#comments'));
      $('.alpha-tabs.simplyTab .alpha-tab-wrapper').wrap("<div class='alpha-tabs-header'/>");
    }else{
      $('#alpha-comment-placer').remove();
    }
  }

  // ===== 最新留言與文章（含標籤） =====
  function initFeedWidgets(){
    $('.alpha-widetize .HTML .widget-content span.alpha-cmnt').each(function(){
      var count = Number($(this).attr('data-no') || 5);
      var target = $(this);
      U.jsonp('/feeds/comments/default?alt=json-in-script&max-results=' + count, function(data){
        var html = '<div class="alpha-comments">';
        if (data.feed.entry){
          data.feed.entry.forEach(function(e){
            var url = '#';
            (e.link || []).forEach(function(l){ if (l.rel === 'alternate') url = l.href; });
            var txt = U.htmlToText(e.content && e.content.$t || e.summary && e.summary.$t || '');
            if (txt.length > 50) txt = txt.substring(0,50) + '...';
            var author = e.author && e.author[0] && e.author[0].name && e.author[0].name.$t || '';
            var avatar = e.author && e.author[0] && e.author[0].gd$image && e.author[0].gd$image.src || '';
            if (avatar.indexOf('img/blank.gif') !== -1 || avatar.indexOf('img/b16-rounded.gif') !== -1){
              avatar = 'https://img1.blogblog.com/img/anon36.png';
            }
            html += '<div class="alpha-comment"><div class="alpha-comment-profile"><img class="alpha-comment-thumb" src="' + avatar + '"/></div><a href="' + url + '">' + author + '</a><span>"' + txt + '"</span></div>';
          });
        }else{
          html += '<div class="alpha-empty-panel bgr"><div class="alpha-empty-thumb"><img src="' + CFG.noImage + '"></div><div class="alpha-empty-text"><h3>No comments!</h3></div></div>';
        }
        html += '</div><div class="clear"></div>';
        target.parent().html(html);
      });
    });

    $('.alpha-widetize .HTML .widget-content span.alpha-recent').each(function(){
      var count = Number($(this).attr('data-no') || 5);
      var target = $(this);
      U.jsonp('/feeds/posts/default?alt=json-in-script&max-results=' + count, function(data){
        var html = '<div class="alpha-recent">';
        if (data.feed.entry){
          data.feed.entry.forEach(function(entry){
            var link = '#';
            (entry.link || []).forEach(function(l){ if (l.rel === 'alternate') link = l.href; });
            var title = entry.title && entry.title.$t || '';
            var author = entry.author && entry.author[0] && entry.author[0].name && entry.author[0].name.$t || '';
            var date = U.formatDate(entry.published && entry.published.$t || '');
            var content = entry.content && entry.content.$t || '';
            var img = CFG.noImage;
            if (content.indexOf('//www.youtube.com/embed/') > -1){
              img = U.normalizeImgUrl(entry.media$thumbnail && entry.media$thumbnail.url || '').replace('/default.jpg','/mqdefault.jpg');
            } else if (content.indexOf('<img') > -1){
              img = U.normalizeImgUrl($('<div>').html(content).find('img:first').attr('src')).replace('/s72-c','/s1600');
            }
            html += '' +
            '<div class="alpha-recent-item">' +
              '<a class="alpha-recent-thumb" href="' + link + '" style="' + U.bgCover(img) + '"><span class="alpha-trap"></span></a>' +
              '<div class="alpha-recent-content">' +
                '<h3 class="alpha-recent-title"><a href="' + link + '">' + title + '</a></h3>' +
                '<span class="alpha-author">' + author + '</span><span class="alpha-time">' + date + '</span>' +
              '</div>' +
            '</div>';
          });
        }else{
          html += '<div class="alpha-empty-panel bgr"><div class="alpha-empty-thumb"><img src="' + CFG.noImage + '"></div><div class="alpha-empty-text"><h3>No Posts found!</h3></div></div>';
        }
        html += '</div>';
        target.parent().html(html);
      });
    });

    $('.alpha-widetize .HTML .widget-content span.alpha-tagswid').each(function () {
      var label = $(this).attr('data-label');
      var count = Number($(this).attr('data-no') || 5);
      var target = $(this);
      if (!label) return;
      U.jsonp('/feeds/posts/default/-/' + label + '?alt=json-in-script&max-results=' + count, function(data){
        var html = '<div class="alpha-recent">';
        if (data.feed.entry){
          data.feed.entry.forEach(function(entry){
            var link = '#';
            (entry.link || []).forEach(function(l){ if (l.rel === 'alternate') link = l.href; });
            var title = entry.title && entry.title.$t || '';
            var cat = entry.category && entry.category[0] && entry.category[0].term || '';
            var author = entry.author && entry.author[0] && entry.author[0].name && entry.author[0].name.$t || '';
            var date = U.formatDate(entry.published && entry.published.$t || '');
            var content = entry.content && entry.content.$t || '';
            var img = CFG.noImage;
            if (content.indexOf('//www.youtube.com/embed/') > -1){
              img = U.normalizeImgUrl(entry.media$thumbnail && entry.media$thumbnail.url || '').replace('/default.jpg','/mqdefault.jpg');
            } else if (content.indexOf('<img') > -1){
              img = U.normalizeImgUrl($('<div>').html(content).find('img:first').attr('src')).replace('/s72-c','/s1600');
            }
            html += '' +
            '<div class="alpha-recent-item">' +
              '<a class="alpha-recent-thumb" href="' + link + '" style="' + U.bgCover(img) + '"><span class="alpha-trap"></span></a>' +
              '<div class="alpha-recent-content">' +
                '<span class="post-cat"><a href="/search/label/' + cat + '?&max-results=' + CFG.perPage + '" title="See all post related to ' + cat + '">' + cat + '</a></span>' +
                '<h3 class="alpha-recent-title"><a href="' + link + '">' + title + '</a></h3>' +
                '<span class="alpha-author">' + author + '</span><span class="alpha-time">' + date + '</span>' +
              '</div>' +
            '</div>';
          });
        }else{
          html += '<div class="alpha-empty-panel bgr"><div class="alpha-empty-thumb"><img src="' + CFG.noImage + '"></div><div class="alpha-empty-text"><h3>No Posts found!</h3></div></div>';
        }
        html += '</div>';
        target.parent().html(html);
      });
    });
  }

  // ===== 其他事件 =====
  function bindMisc(){
    $('.blsm-comment').on('click', function(e){
      e.preventDefault();
      var $placer = $('#alpha-comment-placer');
      if ($placer.length) $('html, body').animate({ scrollTop: $placer.offset().top }, 600);
    });
    initSelectNav('nav');
  }

  // ===== 分頁器（不使用 document.write；JSONP 注入） =====
  (function(){
    var currentPage = null;
    var currentPageNo = 1;
    var postLabel = '';
    var homePage = (window.home_page || '/');
    var urlactivepage = location.href;

    function injectJsonp(url){
      var s = document.createElement('script');
      s.src = url;
      s.async = true;
      document.getElementsByTagName('head')[0].appendChild(s);
    }

    window.totalcountdata = function(root){
      var feed = root.feed;
      var total = parseInt(feed.openSearch$totalResults.$t, 10);
      buildPager(total);
    };

    function buildPager(totalCount){
      var html = '';
      var numPages = window.numPages || 5;
      var pageNumber = parseInt(numPages / 2, 10);
      if (pageNumber === numPages - pageNumber) numPages = pageNumber * 2 + 1;

      var pageStart = currentPageNo - pageNumber;
      if (pageStart < 1) pageStart = 1;

      var lastPageNo = parseInt(totalCount / CFG.perPage, 10) + 1;
      if (lastPageNo - 1 === totalCount / CFG.perPage) lastPageNo = lastPageNo - 1;

      var pageEnd = pageStart + numPages - 1;
      if (pageEnd > lastPageNo) pageEnd = lastPageNo;

      html += "<span class='showpageOf'>Page " + currentPageNo + " of " + lastPageNo + "</span>";

      var prevNumber = currentPageNo - 1;
      if (currentPageNo > 1){
        if (currentPage === 'page'){
          html += '<span class="showpage firstpage"><a href="' + homePage + '"><i class="ion-android-arrow-back"></i> First</a></span>';
        }else{
          html += '<span class="displaypageNum firstpage"><a href="/search/label/' + postLabel + '?&amp;max-results=' + CFG.perPage + '"><i class="ion-android-arrow-back"></i> First</a></span>';
        }
      }

      if (currentPageNo > 2){
        if (currentPageNo === 3){
          if (currentPage === 'page'){
            html += '<span class="showpage"><a href="' + homePage + '"><i class="ion-android-arrow-back"></i></a></span>';
          }else{
            html += '<span class="displaypageNum"><a href="/search/label/' + postLabel + '?&amp;max-results=' + CFG.perPage + '"><i class="ion-android-arrow-back"></i></a></span>';
          }
        }else{
          if (currentPage === 'page'){
            html += '<span class="displaypageNum"><a href="#" data-pager="page" data-no="' + prevNumber + '"><i class="ion-android-arrow-back"></i></a></span>';
          }else{
            html += '<span class="displaypageNum"><a href="#" data-pager="label" data-no="' + prevNumber + '"><i class="ion-android-arrow-back"></i></a></span>';
          }
        }
      }

      if (pageStart > 1){
        if (currentPage === 'page'){
          html += '<span class="displaypageNum"><a href="' + homePage + '">1</a></span>';
        }else{
          html += '<span class="displaypageNum"><a href="/search/label/' + postLabel + '?&amp;max-results=' + CFG.perPage + '">1</a></span>';
        }
      }

      if (pageStart > 2) html += ' ... ';

      for (var jj = pageStart; jj <= pageEnd; jj++){
        if (currentPageNo === jj){
          html += '<span class="pagecurrent">' + jj + '</span>';
        }else if (jj === 1){
          if (currentPage === 'page'){
            html += '<span class="displaypageNum"><a href="' + homePage + '">1</a></span>';
          }else{
            html += '<span class="displaypageNum"><a href="/search/label/' + postLabel + '?&amp;max-results=' + CFG.perPage + '">1</a></span>';
          }
        }else{
          if (currentPage === 'page'){
            html += '<span class="displaypageNum"><a href="#" data-pager="page" data-no="' + jj + '">' + jj + '</a></span>';
          }else{
            html += '<span class="displaypageNum"><a href="#" data-pager="label" data-no="' + jj + '">' + jj + '</a></span>';
          }
        }
      }

      if (pageEnd < lastPageNo - 1) html += '...';

      if (pageEnd < lastPageNo){
        if (currentPage === 'page'){
          html += '<span class="displaypageNum"><a href="#" data-pager="page" data-no="' + lastPageNo + '">' + lastPageNo + '</a></span>';
        }else{
          html += '<span class="displaypageNum"><a href="#" data-pager="label" data-no="' + lastPageNo + '">' + lastPageNo + '</a></span>';
        }
      }

      var nextNumber = currentPageNo + 1;
      if (currentPageNo < (lastPageNo - 1)){
        if (currentPage === 'page'){
          html += '<span class="displaypageNum"><a href="#" data-pager="page" data-no="' + nextNumber + '"><i class="ion-android-arrow-forward"></i></a></span>';
        }else{
          html += '<span class="displaypageNum"><a href="#" data-pager="label" data-no="' + nextNumber + '"><i class="ion-android-arrow-forward"></i></a></span>';
        }
      }

      if (currentPageNo < lastPageNo){
        if (currentPage === 'page'){
          html += '<span class="displaypageNum lastpage"><a href="#" data-pager="page" data-no="' + lastPageNo + '">Last <i class="ion-android-arrow-forward"></i></a></span>';
        }else{
          html += '<span class="displaypageNum lastpage"><a href="#" data-pager="label" data-no="' + lastPageNo + '">Last <i class="ion-android-arrow-forward"></i></a></span>';
        }
      }

      var pageArea = document.getElementsByName('pageArea');
      var blogPager = document.getElementById('blog-pager');
      for (var p=0; p < pageArea.length; p++){ pageArea[p].innerHTML = html; }
      if (pageArea && pageArea.length > 0){ html = ''; }
      if (blogPager){ blogPager.innerHTML = html; }

      // 綁定點擊事件，走 JSONP 導向
      $(document).off('click.enjoyluv.pager','a[data-pager]').on('click.enjoyluv.pager','a[data-pager]', function(e){
        e.preventDefault();
        var type = $(this).data('pager');
        var no = parseInt($(this).data('no'),10);
        if (type === 'page') redirectPage(no);
        else redirectLabel(no);
      });
    }

    function parseContext(){
      var url = urlactivepage;
      if (url.indexOf('/search/label/') !== -1){
        if (url.indexOf('?updated-max') !== -1){
          postLabel = url.substring(url.indexOf('/search/label/') + 14, url.indexOf('?updated-max'));
        } else {
          postLabel = url.substring(url.indexOf('/search/label/') + 14, url.indexOf('?&max'));
        }
      }
      if (url.indexOf('?q=') === -1 && url.indexOf('.html') === -1){
        if (url.indexOf('/search/label/') === -1){
          currentPage = 'page';
          if (urlactivepage.indexOf('#PageNo=') !== -1){
            currentPageNo = parseInt(urlactivepage.substring(urlactivepage.indexOf('#PageNo=') + 8),10);
          } else currentPageNo = 1;
          injectJsonp(homePage + 'feeds/posts/summary?max-results=1&alt=json-in-script&callback=totalcountdata');
        } else {
          currentPage = 'label';
          if (url.indexOf('&max-results=') === -1){
            CFG.perPage = 20; window.perPage = 20; // 舊邏輯相容
          }
          if (urlactivepage.indexOf('#PageNo=') !== -1){
            currentPageNo = parseInt(urlactivepage.substring(urlactivepage.indexOf('#PageNo=') + 8),10);
          } else currentPageNo = 1;
          injectJsonp(homePage + 'feeds/posts/summary/-/' + postLabel + '?alt=json-in-script&callback=totalcountdata&max-results=1');
        }
      }
    }

    function redirectPage(numberpage){
      var jsonstart = (numberpage - 1) * CFG.perPage;
      window.finddatepost = function(root){
        var post = root.feed.entry[0];
        var t1 = post.published.$t.substring(0,19) + post.published.$t.substring(23,29);
        var timestamp = encodeURIComponent(t1);
        var addr = '/search?updated-max=' + timestamp + '&max-results=' + CFG.perPage + '#PageNo=' + numberpage;
        location.href = addr;
      };
      injectJsonp(homePage + 'feeds/posts/summary?start-index=' + jsonstart + '&max-results=1&alt=json-in-script&callback=finddatepost');
    }

    function redirectLabel(numberpage){
      var jsonstart = (numberpage - 1) * CFG.perPage;
      window.finddatepost = function(root){
        var post = root.feed.entry[0];
        var t1 = post.published.$t.substring(0,19) + post.published.$t.substring(23,29);
        var timestamp = encodeURIComponent(t1);
        var addr = '/search/label/' + postLabel + '?updated-max=' + timestamp + '&max-results=' + CFG.perPage + '#PageNo=' + numberpage;
        location.href = addr;
      };
      injectJsonp(homePage + 'feeds/posts/summary/-/' + postLabel + '?start-index=' + jsonstart + '&max-results=1&alt=json-in-script&callback=finddatepost');
    }

    // 對外保留舊 API 名稱以相容可能的外部呼叫
    window.redirectpage = redirectPage;
    window.redirectlabel = redirectLabel;
    window.pagecurrentg = parseContext;

    // 自動啟動分頁器解析
    U.safe(parseContext);
  })();

  // ===== 啟動流程 =====
  $(function(){
    U.safe(buildAlphaMenu);
    U.safe(initMegaMenu);
    U.safe(initFeatured);
    U.safe(relocateWidgets);
    U.safe(normalizeThumbs);
    U.safe(enhanceFeaturedOverlay);
    U.safe(initSideTabs);
    U.safe(initFeedWidgets);
    U.safe(bindMisc);
  });

})(jQuery, window, document);
 //]]>
       </script>
</body>
</html>
