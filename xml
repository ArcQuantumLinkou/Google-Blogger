<b:section class='alpha-fcols-inner alpha-widetize' id='Footer widget(2)' name='Footer Column [2]' preferred='yes'>
   <b:widget id='Translate1' locked='false' title='翻譯' type='Translate'>
     <b:widget-settings>
       <b:widget-setting name='displayMode'>VERTICAL</b:widget-setting>
     </b:widget-settings>
     <b:includable id='main'>
  <b:if cond='data:title != &quot;&quot;'>
    <h2 class='title'><data:title/></h2>
  </b:if>
